    <!-- ======= Add an item ======= -->
    <section class="py-5">

      <p class="createBack" href="#" onclick="window.history.back(); return false;">&lt;Back</p>

      <form style="width: 35rem;" class="mx-auto">

        <!-- Item image -->
        <label class="form-label" for="itemImg">Upload Image:  </label>
        <input type="file" accept="image/jpeg, image/png, image/jpg" id="itemImg" name="img" required><br>

        <!-- Item name -->
        <label class="form-label" for="itemName">Item Name: </label>
        <input type="text" id="itemName" class="form-control" />

        <!-- Item price -->
        <label class="form-label" for="itemPrice">Price: </label>
        <input type="email" id="itemPrice" class="form-control" />

        <!-- Quantity -->
        <label class="form-label" for="itemDescription">Quantity: </label>
        <input type="number" id="itemQuantity" class="form-control" />

        <!-- category -->
        <label class="form-label" for="itemCategory">Category: </label>
        <input type="radio" value="Accessories" class="form-control" />
        <input type="radio" value="Toys" class="form-control" />
        <input type="radio" value="Bowls and Feeders" class="form-control" />
        <input type="radio" value="Carehealth and Supplies" class="form-control" />

        <!-- Item description -->
        <label class="form-label" for="itemDescription">Description: </label>
        <textarea class="form-control" id="itemDescription" rows="4"></textarea>

        <!-- Submit button -->
        <br>
        <button type="button" class="btn btn-primary btn-block mb-4">Submit</button>
      </form>
    </section>

